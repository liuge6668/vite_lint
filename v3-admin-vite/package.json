{
  "name": "v3-vite-ts",
  "type": "module",
  "version": "5.0.2",
  "description": "A crafted admin template, built with Vue3, Vite, TypeScript, Element Plus, and more",
  "scripts": {
    "dev": "vite",
    "build:staging": "vue-tsc && vite build --mode staging",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint . --fix",
    "format": "prettier --write .",
    "stylelint": "stylelint \"**/*.{vue,css,scss,sass,less}\" --fix",
    "prepare": "husky",
    "test": "vitest",
    "check": "vue-tsc --noEmit && eslint . --fix && prettier --write . && stylelint \"**/*.{vue,css,scss,sass,less}\" --fix && vitest run"
  },
  "dependencies": {
    "@element-plus/icons-vue": "2.3.2",
    "axios": "1.13.2",
    "dayjs": "1.11.19",
    "element-plus": "2.12.0",
    "js-cookie": "3.0.5",
    "lodash-es": "4.17.21",
    "mitt": "3.0.1",
    "normalize.css": "8.0.1",
    "nprogress": "0.2.0",
    "path-browserify": "1.0.1",
    "path-to-regexp": "8.3.0",
    "pinia": "3.0.4",
    "screenfull": "6.0.2",
    "vue": "3.5.25",
    "vue-router": "4.6.3",
    "vxe-table": "4.6.25"
  },
  "devDependencies": {
    "@antfu/eslint-config": "6.5.1",
    "@commitlint/cli": "20.3.1",
    "@commitlint/config-conventional": "20.3.1",
    "@types/js-cookie": "3.0.6",
    "@types/lodash-es": "4.17.12",
    "@types/node": "24.10.1",
    "@types/nprogress": "0.2.3",
    "@types/path-browserify": "1.0.3",
    "@vitejs/plugin-vue": "6.0.2",
    "@vue/eslint-config-prettier": "10.2.0",
    "@vue/test-utils": "2.4.6",
    "eslint": "9.39.1",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-format": "1.1.0",
    "happy-dom": "20.0.11",
    "husky": "9.1.7",
    "lint-staged": "16.2.7",
    "prettier": "3.7.4",
    "sass-embedded": "1.78.0",
    "stylelint": "16.26.1",
    "stylelint-config-recommended-vue": "1.6.1",
    "stylelint-config-standard": "39.0.1",
    "stylelint-config-standard-scss": "16.0.0",
    "stylelint-order": "7.0.1",
    "stylelint-scss": "6.14.0",
    "typescript": "5.9.3",
    "unocss": "66.5.10",
    "unplugin-auto-import": "20.3.0",
    "unplugin-svg-component": "0.12.4",
    "unplugin-vue-components": "30.0.0",
    "vite": "7.2.7",
    "vite-plugin-vue-mcp": "0.3.2",
    "vite-svg-loader": "5.1.0",
    "vitest": "4.0.15",
    "vue-tsc": "3.1.5"
  },
  "lint-staged": {
    "*": "eslint --fix",
    "*.{js,ts,vue,json,md}": "prettier --write",
    "*.{vue,css,scss,sass,less}": "stylelint --fix"
  }
}
